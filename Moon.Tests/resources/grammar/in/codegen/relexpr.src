main
  local
    integer x;
    integer y;
    integer z;
  do
    x = -3;
    y = 145;
    z = 7;

    write(x == -3);         // 1
    write(x == x);          // 1
    write(x == y);          // 0

    write(x <> -3);         // 0
    write(x <> x);          // 0
    write(x <> y);          // 1

    write(x < -3);          // 0
    write(x < x);           // 0
    write(x < y);           // 1

    write(x <= -3);         // 1
    write(x <= x);          // 1
    write(x <= y);          // 1

    write(x > -3);          // 0
    write(x > x);           // 0
    write(x > y);           // 0

    write(x >= -3);         // 1
    write(x >= x);          // 1
    write(x >= y);          // 0
  end
